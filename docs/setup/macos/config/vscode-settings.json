{
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "editor.tabSize": 2,
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.folders.color": "#888",
  "typescript.tsserver.log": "off",
  "breadcrumbs.enabled": true,
  "editor.minimap.enabled": false,
  "files.exclude": {
    "**/.git": true
  },
  "markdown-preview-enhanced.liveUpdate": false,
  "eslint.workingDirectories": [
    {"mode": "auto"}
  ],
  "editor.snippetSuggestions": "top",
  "editor.formatOnSave": true,
  "[elixir]": {"editor.formatOnSave": true},
  "[javascript]": {"editor.formatOnSave": false},
  "[json]": {"editor.formatOnSave": false},
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "workbench.colorTheme": "Gruvbox Dark Soft",
  "files.trimTrailingWhitespace": true,
  "git.ignoreMissingGitWarning": true,
  "workbench.editorAssociations": {"git-rebase-todo": "default"},
  "workbench.sideBar.location": "right",
  "git.openDiffOnClick": false,
  "editor.minimap.maxColumn": 98,
  "editor.rulers": [100],
  "html.format.wrapAttributes": "preserve-aligned",
  "html.format.wrapLineLength": 98,
  "elixirLS.suggestSpecs": false,
  "notebook.output.textLineLimit": 300,
  "terminal.external.osxExec": "iterm2.app",
  "window.title": "${activeEditorShort}${separator}${rootNameShort}${separator}${profileName}",
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": "active",
  "workbench.tree.indent": 10,
  "workbench.tree.renderIndentGuides": "always",
  "workbench.colorCustomizations": {"tree.indentGuidesStroke": "#ff582d"},
  "spellright.language": ["en", "fr"],
  "spellright.documentTypes": ["markdown","latex","plaintext"],
  "workbench.activityBar.location": "top",
  "diffEditor.hideUnchangedRegions.enabled": true,
  "explorer.compactFolders": false,
  "window.zoomLevel": 2,
  "security.workspace.trust.untrustedFiles": "open"
}
